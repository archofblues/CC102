#include <iostream>
using namespace std;

int main() {
    char symbol1;
    char symbol2;
    char symbol3;
    int row = 6;
    int column= 10;

    cout << "Enter Symbol 1: ";
    cin >> symbol1;
    cout << "Enter Symbol 2: ";
    cin >> symbol2;
    cout << "Enter Symbol 3: ";
    cin >> symbol3;

    cout << endl;

    for (int i = 0; i < row; i++) {
        for (int j = 0; j < column; j++) {
            cout << symbol2;
        }
        cout << endl;
    }
    
    return 0;
    }
